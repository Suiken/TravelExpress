<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>CheckLogIn</servlet-name>
        <servlet-class>servlets.CheckLogIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckSignIn</servlet-name>
        <servlet-class>servlets.CheckSignIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>servlets.SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Disconnect</servlet-name>
        <servlet-class>servlets.Disconnect</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserPublications</servlet-name>
        <servlet-class>servlets.UserPublications</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Profile</servlet-name>
        <servlet-class>servlets.Profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignIn</servlet-name>
        <servlet-class>servlets.SignIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reservations</servlet-name>
        <servlet-class>servlets.Reservations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileUpdate</servlet-name>
        <servlet-class>servlets.ProfileUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>servlets.Search</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CheckLogIn</servlet-name>
        <url-pattern>/checklogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckSignIn</servlet-name>
        <url-pattern>/checksignin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Disconnect</servlet-name>
        <url-pattern>/disconnect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserPublications</servlet-name>
        <url-pattern>/userpublications</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Profile</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignIn</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reservations</servlet-name>
        <url-pattern>/reservations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileUpdate</servlet-name>
        <url-pattern>/profileupdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
</web-app>